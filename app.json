{
  "expo": {
    "name": "sunbimapp",
    "slug": "sunbimapp",
    "owner": "foilleuse",
    "version": "1.0.0",
    "runtimeVersion": "exposdk:51.0.0", 
    "orientation": "portrait",
    "icon": "./assets/images/nyola.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.foilleuse.sunbimapp",
      "buildNumber": "18",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app does not use location data directly, but third-party libraries may require this permission.",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera to capture clouds.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone for video.",
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos to update your profile picture."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.foilleuse.sunbimapp",
      "versionCode": 16,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-apple-authentication",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to capture clouds.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone for video."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to update your profile picture."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.296503578118-pdqa6300t0r1l315e94nn07uuj8fdepq"
        }
      ],
      [
        "@config-plugins/ffmpeg-kit-react-native",
        {
          "ios": {
            "package": "min-gpl"
          },
          "android": {
            "package": "min-gpl"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6ac7da66-fe81-4d00-b064-035f9535e691"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/6ac7da66-fe81-4d00-b064-035f9535e691"
    }
  }
}